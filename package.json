{
  "name": "create-react-native-plugin",
  "description": "Template for creating React Native plugins without native code.",
  "version": "0.0.0-development",
  "repository": "github:tobua/create-react-native-plugin",
  "license": "MIT",
  "author": "Matthias Giger",
  "padua": {
    "source": true,
    "ignore": [
      "template"
    ]
  },
  "dependencies": {
    "change-case": "5.4.2"
  },
  "devDependencies": {
    "padua": "^2.0.9"
  },
  "type": "module",
  "bin": "index.js",
  "source": "index.js",
  "files": [
    "template",
    "template/.gitignore",
    "*.js"
  ],
  "keywords": [
    "react-native",
    "plugin",
    "template"
  ],
  "prettier": "padua/configuration/.prettierrc.json",
  "eslintConfig": {
    "extends": "./node_modules/padua/configuration/eslint.cjs",
    "rules": {
      "no-console": 0,
      "import/extensions": [
        2,
        "ignorePackages"
      ]
    }
  },
  "publishConfig": {
    "provenance": true
  },
  "engines": {
    "node": ">= 18"
  }
}
