{
  "name": "<%= name %>",
  "version": "1.0.0",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "app": "node create-app.js",
    "watch": "cpx 'src/**/*' app/node_modules/<%= name %>/src --watch",
    "test": "jest --watchAll",
    "lint": "eslint ./src --fix",
    "format": "prettier \"src/**/*\" --write"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "eslintConfig": {
    "extends": "@react-native-community"
  },
  "jest": {
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "/app/"
    ],
    "moduleNameMapper": {
      "<%= name %>": "<rootDir>/src"
    }
  },
  "files": [
    "src"
  ],
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@react-native-community/eslint-config": "0.0.5",
    "babel-jest": "^24.9.0",
    "cpx": "^1.5.0",
    "eslint": "^6.7.2",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.57.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-native": "^0.61.5",
    "react-test-renderer": "^16.12.0",
    "recursive-copy": "^2.0.10",
    "rimraf": "^3.0.0"
  },
  "engines": {
    "node": ">= 14"
  }
}
